print "INT 10";

alias process_table R0;

process_table = PROCESS_TABLE + [SYSTEM_STATUS_TABLE + 1]*16;

[process_table + 4] = TERMINATED;

alias i R1;
i = 1;
while i < MAX_PROC_NUM do
  if [PROCESS_TABLE + i*16 + 4] == TERMINATED then
    i = i + 1;
    continue;
  endif;
  call MOD_5;
endwhile;

halt;